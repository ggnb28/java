package chap07a;

public class PolygonExample {

	public static void main(String[] args) {
		Triangle triangle = new Triangle(50, 80);
		System.out.println("創檜朝 : " + triangle.evaluate());
		Square square = new Square(80, 80);
	    System.out.println("創檜朝 : " + square.evaluate());
//	    SquareB squareB = new SquareB(80);
//	    System.out.println("創檜朝 : " + squareB.evaluate());
	    Trapezoid trapezoid = new Trapezoid(50, 80, 60);
	    System.out.println("創檜朝 : " + trapezoid.evaluate());
				
	}

}
