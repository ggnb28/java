package chap11api;

public class APIExample {

	public static void main(String[] args) {
		// shift 연산은 비트단위 연산
		// 128을 비트패턴 0000 0000 왼쪽으로 세번밀기
		//			  0001 0000 
		// 1번 왼쪽으로 밀때마다 2로 나누는 효과
 	    System.out.println(Integer.toBinaryString(128));
		System.out.println(128 >> 3);
		System.out.println(128 << 3);

	}

}
